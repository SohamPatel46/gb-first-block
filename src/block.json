{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/sp-first",
	"version": "0.1.0",
	"title": "Sp First",
	"category": "text",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"attributes": {
		"message": {
			"type": "string",
			"default": "",
			"selector": "h2",
			"source": "html"
		},
		"message2": {
			"type": "string"
		},
		"textColor": {
			"type": "string"
		},
		"showMessage": {
			"type": "boolean"
		},
		"alignment": {
			"type": "string"
		}
	},
	"supports": {
		"color": {
			"background": true,
			"text": true
		},
		"html": false,
		"typography": {
			"fontSize": true
		}
	},
	"textdomain": "sp-first",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
